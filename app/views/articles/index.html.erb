<h1>Articles</h1>
<ul>
  <% @articles.each do |article| %>
    <li>
    <a href="/articles/<%= article.id %>">
      <strong>Title</strong>: <%= article.title %>
      </a>
      <ul>
      <li>
        <strong>Created At</strong>: <%= article.created_at %>
      </li>
      </ul>
    </li>
    
  <% end %>
</ul>
